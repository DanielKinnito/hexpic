{
  "name": "hexpic",
  "version": "0.1.0",
  "description": "A lightweight TypeScript library for converting images to ASCII art in the browser",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielKinnito/hexpic.git"
  },
  "bugs": {
    "url": "https://github.com/DanielKinnito/hexpic/issues"
  },
  "homepage": "https://github.com/DanielKinnito/hexpic#readme",
  "scripts": {
    "build": "tsc && rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "demo": "npx serve -s",
    "build:demo": "npm run build && node -e \"require('fs').cpSync('dist', 'demo/dist', {recursive: true})\""
  },
  "keywords": [
    "ascii",
    "art",
    "image",
    "converter",
    "typescript",
    "react",
    "component",
    "hook"
  ],
  "author": "Daniel Kinnito",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "react": "^18.0.0",
    "rollup": "^3.0.0",
    "ts-jest": "^29.1.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2"
  }
}